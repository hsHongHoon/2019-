    "def sgnum(a) :\n",
    "    cnt = 0\n",
    "    b = int(a)\n",
    "    # 정수부분이 존재하는 경우, 정수부분이 유효숫자 개수\n",
    "    if int(a):\n",
    "        while b:\n",
    "            b = b/10\n",
    "            b = int(b)\n",
    "            cnt+=1\n",
    "    # 소수부분 유효숫자 개수\n",
    "    while a != 0.0 :\n",
    "        l = len(str(int(a)))\n",
    "        b = float('0'+str(a)[l:])\n",
    "        if b != 0.0 :\n",
    "            cnt+=1\n",
    "        a = a*10\n",
    "        l = len(str(int(a)))\n",
    "        a = float('0'+str(a)[l:])\n",
    "    return cnt"
